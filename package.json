    "export-abi": "hardhat export-abi",
    "clear-abi": "hardhat clear-abi",
    "marketplace-helper": "node  --experimental-specifier-resolution=node --loader ts-node/esm ./scripts/marketplace.ts",
    "passport-helper": "node  --experimental-specifier-resolution=node --loader ts-node/esm ./scripts/passport.ts"
